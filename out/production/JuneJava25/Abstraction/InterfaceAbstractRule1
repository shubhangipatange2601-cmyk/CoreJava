package Abstraction;

 If one abstract class implements one or more interfaces, then it's not mandatory for
 //that child class to provide method implementation for abstract methods of interfaces.
        
        
interface ABC{
    void test();
}

interface WXYZ{
    int add(int a,int b);
}


abstract class InterfaceAbstract  implements ABC,WXYZ {
    @Override
    public int add(int a, int b) {
        return a+b ;
    }

    @Override
    public void test() {
        System.out.println("Test method ");
    }
}
